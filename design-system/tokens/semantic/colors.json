{
  "semantic": {
    "colors": {
      "background": {
        "primary": "var(--color-white)",
        "secondary": "var(--color-gray-50)",
        "tertiary": "var(--color-gray-100)",
        "inverse": "var(--color-gray-950)",
        "overlay": "rgba(0, 0, 0, 0.5)",
        "elevated": "var(--color-white)",
        "sunken": "var(--color-gray-50)",
        "spotlight": "radial-gradient(ellipse at center, rgba(0, 217, 255, 0.1) 0%, transparent 70%)"
      },
      
      "surface": {
        "primary": "var(--color-white)",
        "secondary": "var(--color-gray-50)",
        "tertiary": "var(--color-gray-100)",
        "raised": "var(--color-white)",
        "overlay": "var(--color-white)",
        "interactive": "var(--color-white)",
        "muted": "var(--color-gray-100)"
      },
      
      "text": {
        "primary": "var(--color-gray-900)",
        "secondary": "var(--color-gray-700)",
        "tertiary": "var(--color-gray-500)",
        "disabled": "var(--color-gray-400)",
        "inverse": "var(--color-white)",
        "brand": "var(--color-cyan-500)",
        "link": "var(--color-cyan-600)",
        "error": "var(--color-red-600)",
        "warning": "var(--color-amber-600)",
        "success": "var(--color-emerald-600)",
        "info": "var(--color-blue-600)"
      },
      
      "border": {
        "primary": "var(--color-gray-200)",
        "secondary": "var(--color-gray-300)",
        "tertiary": "var(--color-gray-100)",
