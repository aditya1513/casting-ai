        "error": "var(--color-red-500)",
        "warning": "var(--color-amber-500)",
        "success": "var(--color-emerald-500)",
        "subtle": "var(--color-gray-100)",
        "strong": "var(--color-gray-400)"
      },
      
      "action": {
        "primary": {
          "default": "var(--color-cyan-500)",
          "hover": "var(--color-cyan-600)",
          "active": "var(--color-cyan-700)",
          "disabled": "var(--color-gray-300)"
        },
        "secondary": {
          "default": "var(--color-gray-800)",
          "hover": "var(--color-gray-900)",
          "active": "var(--color-gray-950)",
          "disabled": "var(--color-gray-300)"
        },
        "tertiary": {
          "default": "transparent",
          "hover": "var(--color-gray-100)",
          "active": "var(--color-gray-200)",
          "disabled": "transparent"
        },
        "danger": {
          "default": "var(--color-red-600)",
          "hover": "var(--color-red-700)",
          "active": "var(--color-red-800)",
          "disabled": "var(--color-red-300)"
        },
        "success": {
          "default": "var(--color-emerald-600)",
          "hover": "var(--color-emerald-700)",
          "active": "var(--color-emerald-800)",
          "disabled": "var(--color-emerald-300)"
        }
      },
      
      "status": {
        "error": {
          "background": "var(--color-red-50)",
          "border": "var(--color-red-500)",
          "text": "var(--color-red-700)",
          "icon": "var(--color-red-500)"
        },
        "warning": {
          "background": "var(--color-amber-50)",
          "border": "var(--color-amber-500)",
          "text": "var(--color-amber-700)",
          "icon": "var(--color-amber-500)"
        },
        "success": {
          "background": "var(--color-emerald-50)",
          "border": "var(--color-emerald-500)",
          "text": "var(--color-emerald-700)",
          "icon": "var(--color-emerald-500)"
        },
        "info": {
          "background": "var(--color-blue-50)",
          "border": "var(--color-blue-500)",
          "text": "var(--color-blue-700)",
          "icon": "var(--color-blue-500)"
        }
      },
      
      "brand": {
        "primary": "var(--color-cyan-500)",
        "secondary": "var(--color-purple-500)",
        "accent": "var(--color-pink-500)",
        "gradient": {
          "aurora": "linear-gradient(135deg, var(--color-purple-500) 0%, var(--color-cyan-500) 50%, var(--color-pink-500) 100%)",
          "sunset": "linear-gradient(135deg, var(--color-amber-500) 0%, var(--color-pink-500) 50%, var(--color-purple-600) 100%)",
          "ocean": "linear-gradient(135deg, var(--color-cyan-500) 0%, var(--color-blue-500) 50%, var(--color-blue-900) 100%)"
        }
      }
    }
