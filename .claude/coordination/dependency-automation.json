{
  "version": "2.0",
  "updated": "2025-09-05T01:35:00Z",
  "tracks": {
    "backend_development": {
      "name": "Backend Development Track",
      "agents": ["devops-infrastructure", "backend-api", "ai-ml"],
      "dependencies": [
        {
          "id": "infra-ready",
          "trigger": "redis_operational && postgres_operational",
          "action": "notify_backend_start",
          "payload": {
            "redis_config": "localhost:6379",
            "postgres_config": "localhost:5432",
            "message": "Infrastructure ready - begin Claude integration"
          }
        },
        {
          "id": "claude-api-ready",
          "trigger": "claude_service_responding",
          "action": "notify_frontend_integration",
          "payload": {
            "api_endpoints": ["/api/ai/chat", "/api/ai/stream"],
            "typescript_interfaces": "src/types/ai.d.ts",
            "message": "Claude API ready - connect frontend"
          }
        },
        {
          "id": "memory-system-ready",
          "trigger": "redis_memory_operational",
          "action": "notify_ai_ml_continue",
          "payload": {
            "memory_endpoints": ["/api/memory/store", "/api/memory/retrieve"],
            "message": "Memory system ready - implement advanced features"
          }
        }
      ]
    },
    "design_system": {
      "name": "Design System Track",
      "agents": ["chief-design-officer", "visual-systems", "typography", "layout-grid"],
      "dependencies": [
        {
          "id": "vision-approved",
          "trigger": "cdo_vision_approval",
          "action": "unlock_design_implementation",
          "payload": {
            "approved_vision": "design-system/vision.md",
            "design_tokens": "design-system/tokens.json",
            "message": "Vision approved - begin implementation"
          }
        },
        {
          "id": "tokens-ready",
          "trigger": "design_tokens_90_percent",
          "action": "notify_frontend_design_ready",
          "payload": {
            "token_file": "design-system/tokens.json",
            "component_library": "design-system/components",
            "message": "Design tokens ready - implement UI components"
          }
        }
      ]
    },
    "frontend_implementation": {
      "name": "Frontend Implementation Track",
      "agents": ["frontend-ui", "ux-architect"],
      "dependencies": [
        {
          "id": "backend-api-available",
          "trigger": "backend_api_operational",
          "action": "begin_api_integration",
          "payload": {
            "api_base": "http://localhost:3001",
            "auth_endpoints": ["/api/auth/login", "/api/auth/register"],
            "message": "Backend ready - integrate APIs"
          }
        },
        {
          "id": "design-system-available",
          "trigger": "design_tokens_ready",
          "action": "implement_design_system",
          "payload": {
            "design_tokens": "frontend/styles/tokens.css",
            "components": "frontend/components/ui",
            "message": "Design system ready - apply to components"
          }
        }
      ]
    }
  },
  "automation_rules": {
    "blocker_detection": {
      "check_interval": 7200,
      "rules": [
        {
          "condition": "redis_down > 4_hours",
          "action": "escalate_to_devops_lead",
          "severity": "critical"
        },
        {
          "condition": "api_response_time > 3_seconds",
          "action": "notify_backend_optimization",
          "severity": "high"
        },
        {
          "condition": "frontend_build_failed",
          "action": "rollback_and_notify",
          "severity": "medium"
        }
      ]
    },
    "handoff_protocols": {
      "backend_to_frontend": {
        "trigger": "api_endpoint_complete",
        "validation": ["swagger_docs_updated", "typescript_types_generated"],
        "notification": {
          "to": ["frontend-ui-developer"],
          "includes": ["endpoint_url", "request_format", "response_format", "auth_required"]
        }
      },
      "design_to_frontend": {
        "trigger": "component_design_approved",
        "validation": ["figma_link", "design_tokens", "accessibility_checklist"],
        "notification": {
          "to": ["frontend-ui-developer"],
          "includes": ["component_specs", "interaction_patterns", "responsive_breakpoints"]
        }
      },
      "ai_to_backend": {
        "trigger": "ai_service_ready",
        "validation": ["api_docs", "performance_metrics", "error_codes"],
        "notification": {
          "to": ["backend-api-developer"],
          "includes": ["service_url", "authentication", "rate_limits"]
        }
      }
    },
    "quality_gates": {
      "pre_integration": {
        "checks": ["unit_tests_pass", "linting_pass", "type_check_pass"],
        "approval_required": false
      },
      "pre_deployment": {
        "checks": ["integration_tests_pass", "performance_benchmarks_met", "security_scan_pass"],
        "approval_required": true,
        "approvers": ["task-completion-enforcer", "design-review-qa"]
      }
    }
  },
  "current_state": {
    "active_dependencies": [
