{
  "name": "CastMatch - Elite Casting Platform",
  "short_name": "CastMatch",
  "description": "AI-powered casting platform connecting talent with opportunities in Mumbai's entertainment industry",
  "start_url": "/?source=pwa",
  "id": "com.castmatch.app",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "background_color": "#000000",
  "theme_color": "#8b5cf6",
  "orientation": "any",
  "categories": ["business", "entertainment", "productivity"],
  "lang": "en-IN",
