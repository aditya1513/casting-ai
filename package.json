{
  "name": "castmatch-backend",
  "version": "1.0.0",
  "description": "CastMatch - AI-powered casting platform backend for Mumbai OTT industry",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "node dist/server.js",
    "monitor": "ts-node src/monitoring/index.ts",
    "monitor:cli": "ts-node src/scripts/monitor-agents.ts",
    "monitor:dashboard": "ts-node src/monitoring/real-time-dashboard.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,js,json}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit --coverage --coverageDirectory=coverage/unit",
    "test:integration": "jest --testPathPattern=integration --coverage --coverageDirectory=coverage/integration",
    "test:security": "jest --testPathPattern=security --coverage --coverageDirectory=coverage/security",
    "test:api": "jest --testPathPattern=api --coverage --coverageDirectory=coverage/api",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "node tests/performance/performance.test.js",
    "test:artillery": "artillery run tests/performance/artillery-config.yml",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security && npm run test:api && npm run test:e2e",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "castmatch",
    "casting",
    "ott",
    "mumbai",
    "talent",
    "ai"
  ],
  "author": "CastMatch Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
